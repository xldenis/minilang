var a : int;
var b : int;
var c : int;
var d : int;
var loopCond : int;


loopCond = 1;

read a;
read b;
c = a - b;
d = a - b;
while loopCond do
  if c then
    c = c;
  else
    loopCond = 0;
  endif
  if d then
    d = d;
  else
    loopCond = 0;
  endif

  c = c + 1;
  d = d - 1;
done

if c - d then
  if c then 
    if d then
      print 1;
    else
      print 0;
    endif
  else
    print 0;
  endif
else
  print 0;
endif